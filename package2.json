{
  "name": "packing_list",
  "author": "Evgeniy Tatarin, Sergii Milkovskyi, Anton Kalmykov, Ivan Strelcov, Bohdan Vlasov",
  "version": "0.0.1",
  "description": "frontend part",
  "babel": {
    "presets": [
      [
        "es2015"
      ],
      "react",
      "stage-0"
    ],
    "plugins": [
      [
        "transform-decorators-legacy",
        "transform-class-properties",
        "transform-object-rest-spread"
      ]
    ],
    "env": {
      "start": {
        "presets": [
          "react-hmre"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "mocha": true
    },
    "ecmaFeatures": {
      "modules": true,
      "es6": true,
      "jsx": true,
      "browser": true
    },
    "settings": {
      "import/resolver": "webpack"
    },
    "globals": {
      "connect": true
    },
    "rules": {
      "class-methods-use-this": 0,
      "react/sort-comp": 0,
      "max-len": 0,
      "react/jsx-uses-react": 2,
      "react/jsx-uses-vars": 2,
      "react/react-in-jsx-scope": 2,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "block-scoped-var": 0,
      "padded-blocks": 0,
      "no-console": 0,
      "id-length": 0,
      "no-unused-expressions": 0,
      "react/jsx-curly-spacing": [
        2,
        "always",
        {
          "allowMultiline": false
        }
      ]
    },
    "plugins": [
      "react"
    ]
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "webpack --progress --verbose --colors --display-error-details --config webpack/common.config.js",
    "build:production": "npm run clean && npm run build",
    "lint": "eslint src",
    "start": "./node_modules/.bin/webpack-dev-server",
    "test": "mocha-webpack --webpack-config webpack.config-test.js ./test/actions/ui/wizard.spec.js"
  },
  "devDependencies": {
    "asset-require-hook": "^1.0.2",
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.7.2",
    "babel-eslint": "^7.2.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-remove-prop-types": "^2.0.2",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-plugin-transform-require-ignore": "0.0.2",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-plugin-webpack-alias": "^1.3.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-3": "^6.3.13",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "bootstrap-loader": "^1.0.2",
    "bootstrap-sass": "^3.3.6",
    "css-loader": "0.14.5",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "expect": "^1.12.0",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "history": "1.13.1",
    "html-webpack-plugin": "^2.24.1",
    "imports-loader": "^0.6.4",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^9.12.0",
    "json-loader": "^0.5.4",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "mocha": "^3.2.0",
    "mocha-webpack": "^0.7.0",
    "nock": "^9.0.9",
    "node-sass": "^3.13.0",
    "postcss-import": "^7.1.3",
    "postcss-loader": "^1.1.1",
    "prepush": "^3.1.11",
    "react-addons-css-transition-group": "^0.14.2",
    "react-addons-test-utils": "^15.4.2",
    "react-document-meta": "^2.0.0-rc2",
    "react-dom": "^0.14.8",
    "react-hot-loader": "^1.2.7",
    "react-loading-order-with-animation": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.1.1",
    "redux-mock-store": "^1.2.2",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.1.0-beta.10",
    "webpack-hot-middleware": "^2.4.1",
    "webpack-merge": "^0.7.1"
  },
  "dependencies": {
    "array-includes": "^3.0.2",
    "axios": "^0.15.3",
    "babel-polyfill": "^6.3.14",
    "bcryptjs": "^2.4.0",
    "es6-promise": "^4.0.5",
    "moment": "^2.17.1",
    "normalize": "^0.3.1",
    "normalize.css": "^5.0.0",
    "normalizr": "^2.2.1",
    "react": "^15.4.2",
    "react-day-picker": "^3.1.1",
    "react-dom": "^15.4.2",
    "react-modal-dialog": "^4.0.3",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.0-rc4",
    "react-router-redux": "^4.0.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^1.0.3",
    "redux": "^3.0.4",
    "roboto-fontface": "^0.6.0",
    "webpack-node-externals": "^1.5.4"
  }
}

    "postcss-loader": "^1.1.1",
    "react-addons-test-utils": "^15.4.2",
    "react-scripts": "0.9.4",
    "redux-logger": "^3.0.0",
    "sass-loader": "^4.0.2",
    "sinon": "^2.1.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.1.0-beta.10",
    "webpack-hot-middleware": "^2.4.1",
    "webpack-merge": "^0.7.1"
  },
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server",
    "build": "webpack --progress --verbose --colors --display-error-details --config webpack/common.config.js",
    "lint": "eslint src",
    "test": "mocha \"./src/**/*.spec.js\" --require babel-register",
    "json-server": "./node_modules/.bin/json-server --watch ./db.json --port 3001"
